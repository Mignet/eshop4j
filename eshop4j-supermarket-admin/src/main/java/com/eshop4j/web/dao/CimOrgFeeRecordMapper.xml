	<!-- 收费记录批量插入 -->
	<insert id="insertBatchFee" parameterType="java.util.List">
	   INSERT INTO tcim_org_fee_record
	   (
					org_number,
					fee_rule_id,
					fee_ratio,
					fee_val,
					interval_min_val,
					interval_max_val,
					interval_unit,
					creattime,
					creator,
					updatetime,
					updater,
					remark
		)
		values
	    <foreach collection="list" item="feeRecord" index="index" separator="," >
	        (
					 	#{feeRecord.orgNumber},
					 	#{feeRecord.feeRuleId},
					 	#{feeRecord.feeRatio},
					 	#{feeRecord.feeVal},
					 	#{feeRecord.intervalMinVal},
					 	#{feeRecord.intervalMaxVal},
					 	#{feeRecord.intervalUnit},
					 	#{feeRecord.creattime},
