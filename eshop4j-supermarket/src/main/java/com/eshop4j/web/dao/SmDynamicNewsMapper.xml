	<select id="queryNetloanNewsPageList" resultType="com.eshop4j.api.response.NetloanNewsResponse" >
		SELECT
			tc.summary AS summary,
			tc.title AS title,
			tc.link_to AS linkUrl,
			tc.text AS content,
			tc.module AS module,
			tc.slug AS slug,
			tc.id AS id,
			tc.created AS created,
			tc.thumbnail AS thumbnail
		FROM
			tjp_taxonomy tt,
			tjp_mapping tp,
			tjp_content tc
		WHERE
			tt.title = #{title}
		AND tt.type = 'category'
		AND tt.id = tp.taxonomy_id
		AND tp.content_id = tc.id
		AND tc.status = 'normal'
		order by tc.created desc
	</select>
	
